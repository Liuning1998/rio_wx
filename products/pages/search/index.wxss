/* products/pages/index/index.wxss */
page {
  background: #f7f7f7;
  height: 100%;
}

.search-container {
  position: fixed;
  left: 0;
  width: 750rpx;
  height: 44px;
  background: #EBEBEB;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 3;
}

.search-container .cancel {
  font-size: 28rpx;
  color: #666666;
  padding-left: 24rpx;
}

.search-container .input-group {
  display: flex;
  align-items: center;
  /* margin-left: 24rpx; */
  box-sizing: border-box;
  width: 622rpx;
  height: 52rpx;
  background: #ffffff;
  border-radius: 26rpx;
  padding: 0 16rpx;
}

.search-container .input-group .placeholder {
  font-size: 28rpx;
  color: #999999;
}

.search-container .input-group input {
  flex: 1;
  padding: 0 16rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #333333;
  height: 52rpx;
  line-height: 52rpx;
}

.search-container .search-icon {
  width: 32rpx;
  height: 32rpx;
  flex-shrink: 0;
}

.search-container .close {
  height: 28rpx;
  width: 28rpx;
  background:  rgba(0,0,0,0.20);
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 10rpx 4rpx;
}

.search-container .close-icon {
  height: 14rpx;
  width: 16rpx;
  flex-shrink: 0;
}

.list-cations-header {
  position: fixed;
  left: 0;
  width: 750rpx;
  height: 104rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background: #FFFFFF;
  z-index: 3;
}

.list-cations-header .action-item {
  display: flex;
  align-items: center;
  font-family: PingFangSC-Medium;
  font-size: 40rpx;
  color: #332B2B;
}

.list-cations-header .action-item.active {
  color: #FF3633;
}

.list-cations-header .price-arrow {
  width: 10rpx;
  margin-left: 8rpx;
  padding-top: 2rpx;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
}

.list-cations-header .price-arrow .up {
  width: 12rpx;
  height: 8rpx; 
  margin-bottom: 8rpx;
}

/* .list-cations-header .price-arrow.up .up {
  border-color: transparent transparent #CE343C transparent;
} */

.list-cations-header .price-arrow .down {
  width: 12rpx;
  height: 8rpx;
  transform: rotate(180deg);
}

/* .list-cations-header .price-arrow.down .down {
  border-color: #CE343C transparent transparent transparent;
} */

.products-container {
  padding-top: 132rpx;
  padding-bottom: 100rpx;
  box-sizing: border-box;
}

.product{
  display: flex;
  width: 750rpx;
  padding: 28rpx 24rpx 0;
  box-sizing: border-box;
  background-color: #Fff;
  position: relative;
}
.product .line {
  position: absolute;
  width: 100%;
  height: 1rpx;
  background: #f7f7f7;
  bottom: 0;
  right: 0;
}
.product .avatar-container{
  padding-bottom: 28rpx;
}
.product .avatar-container image{
  width: 272rpx;
  height: 272rpx;
  display: flex;
  border-radius: 6rpx;
  margin-right: 24rpx;
  border: 2rpx solid #E6E6E6;
}
.product .product-info{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  flex: 1;
  position: relative;
}

.product .product-info .name-and-badge .pre_name{
  color: #F64C47;
}
.product .product-info .price-and-btn{
  display: flex;
  width: 100%;
  justify-content: space-between;
  align-items: center;
  line-height: 1;
  padding-bottom: 14rpx;
}
.product .product-info .price-and-btn .price-container{
  margin-top: 14rpx;
}
.product .product-info .price-and-btn .unit{
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #FF3633;
  font-weight: 400;
}
.product .product-info .price-and-btn .price{
  font-family: PingFangSC-Medium;
  font-size: 40rpx;
  color: #FF3633;
  font-weight: 500;
}
 .product .product-info .price-and-btn .add-cart{
  width: 72rpx;
  height: 72rpx;
}


.products-container .avatar-container {
  flex-shrink: 0;
  border-radius: 8rpx;
  overflow: hidden;
}


.products-container .item-content {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  flex: 1;
}

.products-container .name {
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  display: -webkit-box;
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #332B2B;
  font-weight: 400;
  line-height: 44rpx;
}

.products-container .pbadges{
  padding-top: 0;
}

.products-container .name .pre_name{
  color: #F64C47;
}

.products-container .description {
  font-size: 24rpx;
  color: #D3B17D;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  display: -webkit-box;
  box-sizing: border-box;
}



/* 空列表样式 */
.product-empty {
  width: 750rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  background: #f7f7f7;
  height: 100%;
}

.product-empty image {
  width: 336rpx;
  height: 270rpx;
  margin-top: 230rpx;
}

.product-empty text {
  font-size: 28rpx;
  color: #333333;
  margin-top: 30rpx;
}
/* 空列表样式 */

/* 新navbar s */
.navbar{
  display: flex;
  flex-direction: column;
  background: #fff;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 999;
  width: 100%;
}
.navbar .navbar-content{
  width: 100%;
  height: 44px;
  line-height: 44px;
  display: flex;
  position: relative;
  justify-content: flex-start;
  align-items: center;
  z-index: 1;
  border-bottom: 1rpx solid rgba(230,230,230,1);
}
.navbar .navbar-content .return-box{
  display: flex;
  align-items: center;
  justify-content: center;
  height: 44px;
  padding-left: 30rpx;
  padding-right: 22rpx;
}
.navbar .navbar-content .return-box image{
  height: 36rpx;
  width: 20rpx;
}
.navbar .navbar-content .search-box{
  width: 474rpx;
  height: 64rpx;
  display: flex;
  background: #F5F5F5;
  border: 2rpx solid #E6E6E6;
  border-radius: 36rpx;
  align-items: center;
  justify-content: flex-start;
  padding: 0 12rpx;
  box-sizing: border-box;
}
.navbar .navbar-content .search-box .search-content{
  height: 52rpx;
  background: rgba(17,17,17,0.50);
  border-radius: 42rpx;
  padding: 0 20rpx;
  display: flex;
  align-items: center;
}
.navbar .navbar-content .search-box .search-content .search-key{
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #FFFFFF;
  margin-right: 12rpx;
  line-height: normal;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  display: -webkit-box;
  box-sizing: border-box;
}
.navbar .navbar-content .search-box .search-content .close{
  width: 24rpx;
  height: 24rpx;
  flex: none;
}
.navbar .navbar-content .search-box .text{
  width: 100%;
  padding: 0 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  display: -webkit-box;
  box-sizing: border-box;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #999999;
  font-weight: 400;
}
/* 新navbar e */

/* 搜索层 */

.search-layer-1 {
  position: fixed;
  background: rgba(0,0,0,0.15);
  width: 750rpx;
  height: 100%;
  z-index: 9951;
}

.search-layer-1 .search-container-01 {
  width: 750rpx;
  background: #ffffff;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  border-radius: 32rpx;
  overflow: hidden;
}

.search-layer-1 .search-title {
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 36rpx;
  color: #2B2424;
  font-weight: 600;
  font-size: 36rpx;
  height: 44px;
  line-height: 44px;
  box-sizing: border-box;
}

.search-layer-1 .icon {
  height: 28rpx;
  width: 28rpx;
  display: flex;
  margin-right: 8rpx;
}

.search-layer-1 .search-container {
  width: 750rpx;
  background: #ffffff;
  display: flex;
  align-items: center;
  height: 112rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
}

.search-layer-1 .input-group {
  display: flex;
  align-items: center;
  flex: 1;
  width: 622rpx;
  height: 64rpx;
  background: #f7f7f7;
  border-radius: 32rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.search-layer-1 .input-group input {
  flex: 1;
  font-size: 28rpx;
  color: #2B2424;
}

.search-layer-1 .input-group .clear{
  height: 60rpx;
  width: 60rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  position: relative;
  box-sizing: border-box;
  right: -10rpx;
}

.search-layer-1 .input-group .clear text {
  display: inline-block;
  width: 28rpx;
  height: 28rpx;
  background: #B4AFAF;
  color: #ffffff;
  font-size: 20rpx;
  line-height: 26rpx;
  text-align: center;
  border-radius: 50%;
  margin-right: 10rpx;
}

.search-layer-1 .cancel-btn {
  font-size: 28rpx;
  color: #5B5454;
  flex-shrink: 0;
  padding-left: 24rpx;
}
/* 搜索层 */