/* 横向推荐 */
.brand-transverse{
  width: 702rpx;
  margin: 36rpx auto 0 auto;
  background: #ffffff;
  padding: 24rpx;
  box-sizing: border-box;
  border-radius: 16rpx;
}
.brand-transverse .brand-title{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
}
.brand-transverse .brand-title .left{
  font-size: 30rpx;
  font-weight: 500;
}
.brand-transverse .brand-title .right{
  font-size: 20rpx;
  color: #999;
}
.brand-scroll .product-item-contaier{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 245rpx;
  padding: 16rpx;
  margin-right: 20rpx;
  box-sizing: border-box;
  padding-bottom: 28rpx;
  border-radius: 14rpx;
  background: #f7f7ff;
  margin-bottom: 16rpx;
}
.brand-scroll  .avatar {
    height: 213rpx;
    width: 213rpx;
    display: flex;
}
.brand-scroll .product-content .name-container {
  flex: 1;
}
.brand-scroll .product-content .name {
  font-size: 28rpx;
  color: #2B2424;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  display: -webkit-box;
}
.brand-scroll .price-and-btn {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 15rpx;
}
.brand-scroll .price-container {
  display: flex;
  align-items: center;
  display: flex;
  align-items: center;
  max-width: calc(100% - 55rpx);
  flex-wrap: wrap;
}
.brand-scroll .price {
  font-size: 28rpx;
  color: #F64C47;
  font-weight: 500;
  margin-right: 8rpx;
  width: 100%;
}
.brand-scroll .pbadges{
  margin-top: 15rpx;
}
.brand-scroll .price-decimal {
  font-size: 20rpx;
  color: #F64C47;
  font-weight: 500;
  margin-right: 8rpx;
}
.brand-scroll  .price::before {
  content: '￥';
  font-size: 20rpx;
  font-weight: 400;
  margin-bottom: 4rpx;
}
.brand-scroll .price-decimal::before {
  content: '.';
}
.brand-scroll  .origin-price {
  font-size: 20rpx;
  color: #B4AFAF;
  text-decoration: line-through;
  margin-top: 4rpx;
}
.brand-scroll  .add-cart-image{
  width: 48rpx;
  height:48rpx;
}
.brand-scroll  .add-cart-image-box{
  display: flex;
  align-items: flex-end;
  height: 100%;
  padding-bottom: 10rpx;
}
.brand-scroll .product-content{
  width: 100%;
  display: flex;
  flex-direction: column;
  flex: 1;
}
.brand-scroll  .add-cart {
  /* background: #F64C47; */
  display: inline-block;
  height: 48rpx;
  width: 48rpx;
  border-radius: 24rpx;
  color: #ffffff;
  line-height: 40rpx;
  text-align: center;
  font-size: 42rpx;
}
.brand-scroll{
  white-space: nowrap;
}
.brand-more{
  border-radius: 16rpx;
  background-color: #f7f7ff;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  padding: 102rpx 34rpx;
  color: #999;
  margin-bottom: 16rpx;
  box-sizing: border-box;
  flex-wrap: wrap;
  width: 100rpx;
  text-align: center;
}
.brand-more .brand-more-icon{
  width: 16rpx;
  height:26rpx;
  transform: rotate(180deg);
}
.brand-more .brand-more-text{
  white-space: pre-wrap;
}
.brand-scroll-container{
  width: 100%;
  display: flex;
  flex-wrap: nowrap;
}
/* 横向推荐 */