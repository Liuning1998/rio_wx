/* 用户信息、轮播图 */
.swiper-and-user {
  width: 750rpx;
  position: relative;
  overflow: hidden;
}

.swiper-and-user .su-background {
  height: 372rpx;
  width: 750rpx;
  position: absolute;
  /* bottom: 0; */
  z-index: 3;
}

.su-container {
  position: relative;
  z-index: 5;
}

.su-container.padding-top-88 {
  padding-top: 25rpx;
}

.swiper-and-user .user-info {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 32rpx;
  padding-top: 1rpx;
  box-sizing: border-box;
  display: none;
}

.swiper-and-user .account-info {
  flex: 1;
  flex-shrink: 0;
  display: flex;
}

.swiper-and-user .account-info .balance {
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  display: -webkit-box;
  width: 304rpx;
  font-size: 28rpx;
  color: #ffffff;
  margin-left: 10rpx;
  font-weight: 400;
  flex-shrink: 0;
  background: rgba(255,255,255,0.10);
  border-radius: 4px;
  border-radius: 4px;
  height: 80rpx;
  line-height: 80rpx;
  flex: 1;
  padding: 0 16rpx;
  font-weight: 500;
}

.swiper-and-user .user-info .avatar-container {
  height: 76rpx;
  width: 76rpx;
  overflow: hidden;
  border-radius: 38rpx;
  margin-right: 10rpx;
}

.swiper-and-user .user-info .avatar-image {
  height:  76rpx;
  width: 76rpx;
  display: flex;
}

.swiper-and-user .avatar-and-name {
  display: flex;
  align-items: center;
  margin-right: 30rpx;
}

.swiper-and-user .user-info .name {
  font-size: 28rpx;
  color: #ffffff;
  font-weight: 500;
}

.swiper-and-user .defualt-user {
  display: flex;
  align-items: center;
  width: 750rpx;
  padding: 0 32rpx;
  box-sizing: border-box;
}

.swiper-and-user .defualt-user .default-avatar {
  height: 76rpx;
  width: 76rpx;
  display: flex;
  border-radius: 38rpx;
  flex-shrink: 0;
  margin-right: 10rpx;
}

.swiper-and-user .defualt-user text {
  background: rgba(255,255,255,0.10);
  border-radius: 4px;
  border-radius: 4px;
  display: inline-block;
  height: 80rpx;
  line-height: 80rpx;
  flex: 1;
  font-size: 28rpx;
  color: #ffffff;
  padding: 0 16rpx;
}

.swiper-and-user .swiper-container {
  width: 750rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
  margin-top: 40rpx;
}

.swiper-and-user .swiper-content {
  width: 702rpx;
  height: 220rpx;
  border-radius: 8rpx;
  overflow: hidden;
}

.swiper-and-user swiper, .swiper-and-user swiper-item, .swiper-and-user .slide-image-container {
  width: 702rpx;
  height: 220rpx;
  border-radius: 8rpx;
  overflow: hidden;
  transform: translateY(0);
}

.swiper-and-user .slide-image {
  width: 702rpx;
  height: 220rpx;
}

.swiper-dots-container {
  position: absolute;
  left: 0;
  bottom: 24rpx;
  width: 750rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 82;
}

.swiper-dots-container .dot {
  width: 6px;
  height: 3px;
  background: rgba(255,255,255,0.50);
  box-sizing: border-box;
  margin: 0 3px;
  border-radius: 1rpx;
}

.swiper-dots-container .dot.active {
  width: 24rpx;
  background: #ffffff;
}

.special-area-swiper-dots-container {
  position: absolute;
  left: 0;
  bottom: 0rpx;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 82;
}

.special-area-swiper-dots-container .dot {
  width: 6px;
  height: 3px;
  background: #dddddd;
  box-sizing: border-box;
  margin: 0 3px;
  border-radius: 1rpx;
}

.special-area-swiper-dots-container .dot.active {
  width: 24rpx;
  background: #fa2c19;
}
