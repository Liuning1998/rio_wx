page {
  background: #fff;
}

.banner-container {
  display: flex;
  justify-content: space-between;
  padding: 0 0 0 32rpx;
}
.banner-container .dialog{
  margin-top: 64rpx;
  width: 401.28rpx;
  height: 116rpx;
  display: flex;
  justify-content: center;
  padding: 0 0 0 28rpx;
  flex-direction: column;
  position: relative;
  box-sizing: border-box;
}
.banner-container .dialog .img{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
}
.banner-container .dialog .dialog-text{
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #FF3633;
  font-weight: 400;
  position: relative;
}
.banner-container .banner-bg{
  width: 284rpx;
  height: 198rpx;
  margin-right: 28rpx;
}
.title{
  width: 750rpx;
  background-color: #fff;
  height: 96rpx;
  display: flex;
  align-items: center;
  padding: 0 32rpx;
  box-sizing: border-box;
}
.title text{
  font-family: PingFangSC-Medium;
  font-size: 40rpx;
  color: #333333;
  font-weight: 500;
}
.tab .tab-scroll{
  width: 750rpx;
  display: flex;
  justify-content: space-between; 
  padding: 16rpx 48rpx 0;
  box-sizing: border-box;
}
.tab .tab-scroll .tab-list{
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
  padding-bottom: 40rpx;
  overflow: hidden;
}
.tab .tab-scroll .tab-list .tab-list-img{
  width: 96rpx;
  height: 96rpx;
  margin-bottom: 20rpx;
}
.tab .tab-scroll .tab-list .icon{
  position: absolute;
  bottom: -12rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 28rpx;
  height: 12rpx;
  transition: all .2s;
}
.tab .tab-scroll .tab-list.on .icon{
  bottom: 0;
}
.tab .tab-scroll .tab-list text{
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #666666;
  letter-spacing: 0;
  line-height: normal;
  font-weight: 400;
}
.tab .tab-content{
  height: 100%;
}
.tab .tab-content .tab-content-list{
  width: 686rpx;
  margin: 0 auto;
  border-bottom: 1rpx solid rgba(229,229,229,1);
  min-height: 66rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 0;
}
.tab .tab-content .tab-content-list:last-child{
  border-bottom:none;
}
.line{
  width: 100%;
  height: 16rpx;
  background-image: linear-gradient(0deg, #FFFFFF 0%, #f3f3f3 100%);
}
.tab .tab-content .tab-content-list .dot{
  width: 8rpx;
  height: 8rpx;
  background: #E5E5E5;
  border-radius: 50%;
  margin-right: 16rpx;
}
.tab .tab-content .tab-content-list .text{
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #666666;
  font-weight: 400;
  line-height: 44rpx;
  /* white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis; */
  max-width: 570rpx;
}
.tab .tab-content .tab-content-list .arrow{
  flex: 1;
  display: flex;
  justify-content: flex-end;
}
.tab .tab-content .tab-content-list .arrow image{
  width: 60rpx;
  height: 60rpx;
}

.tokefu{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  background: #FFFFFF;
  box-shadow: 0rpx -0.5rpx 1rpx 0rpx rgba(229,229,229,1);
}
.tokefu button{
  width: 686rpx;
  height: 96rpx;
  background-image: linear-gradient(123deg, #FF511E 0%, #FC1000 100%);
  border-radius: 54rpx;
  margin: 14rpx auto 12rpx;
  font-family: PingFangSC-Medium;
  font-size: 36rpx;
  color: #FFFFFF;
  font-weight: 500;
  padding: 0;
  display: flex;
  align-items: center;
  justify-content: center;
}
.tokefu text{
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #FF3633;
  letter-spacing: 0;
  font-weight: 400;
  text-align: center;
  display: block;
  margin-bottom: 22rpx;
}
.tokefu.iphone-x {
  padding-bottom: 68rpx;
}