  font-size: 26rpx;
  margin-right: 24rpx;
  margin-top: 1px;
  line-height: 56rpx;
  border-top: 1rpx solid #f7f7ff;
}
.coupon-box{
  display: block;
  min-height: 88rpx;
  height: auto;
}
.coupon-box .content{
  height: 88rpx;
}
.coupon-box-down{
  display: flex;
  align-items: center;
}
.coupon-box-down.show image{
  transform: rotate(-90deg);
}
.coupon-box-down image{
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  transform: rotate(90deg);
  transition: all .2s;
}
