/* 订单信息 */

.bottom-btn-group {
  position: fixed;
  bottom: 0;
  left: 0;
  background: #ffffff;
  z-index: 9;
  box-shadow: 0 9px 28px -8px rgba(0,0,0,0.50);
}
.bottom-btn-group.iphone-x {
  padding-bottom: 68rpx;
}
.bottom-btn-group .btn-content {
  height: 100rpx;
  width: 750rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  background-color: #ffffff;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.bottom-btn-group .delete {
  height: 28rpx;
  width: auto;
}
.bottom-btn-group .btn.buy, .bottom-btn-group .btn.pay{
  display: inline-block;
  text-align: center;
  height: 60rpx;
  line-height: 58rpx;
  width: 180rpx;
  color: #F64C47;
  font-size: 28rpx;
  border-radius: 30rpx;
  box-sizing: border-box;
  margin-left: 20rpx;
  border: 1px solid #F64C47;
}

.bottom-btn-group .btn.pay{
  background: #F64C47;
  color: #ffffff;
}

.bottom-btn-group .btn.cancel, .bottom-btn-group .btn.delete{
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  height: 60rpx;
  line-height: 58rpx;
  width: 208rpx;
  /* background: #e23c42; */
  color: #5B5454;
  font-size: 28rpx;
  border-radius: 30rpx;
  box-sizing: border-box;
  border: 1px solid #5B5454;
}

.bottom-btn-group .btn.cancel {
  margin-left: 20rpx;
}

.bottom-btn-group .btn:active {
  opacity: 0.75;
}

.goto-coupons {
  height: 38rpx;
  line-height: 34rpx;
  border-radius: 19rpx;
  width: 136rpx;
  text-align: center;
  display: inline-block;
  color: #C0780A;
  border: 2rpx solid #C0780A;
  box-sizing: border-box;
}

/* 支付提示 notice */
.pay-notice-container {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 199980;
  height: 100%;
  width: 750rpx;
  background: rgba(0, 0, 0, 0.2);
  display: flex;
  align-items: center;
  justify-content: center;
}

.pay-notice-content {
  width: 630rpx;
  background: #ffffff;
  border-radius: 8rpx;
  display: flex;
  flex-direction: column;
  min-height: 400rpx;
}

.pay-notice-content .pn-header {
  border-bottom: 1px solid #eeeeee;
  height: 88rpx;
  width: 630rpx;
  position: relative;
}

.pay-notice-content .pn-header .close {
  /* width: 24rpx;
  height: 20rpx; */
  position: absolute;
  right: 4rpx;
  top: 12rpx;
  font-size: 40rpx;
  color: #666666;
  padding: 0rpx 30rpx;
}

.pay-notice-content .pn-content {
  display: flex;
  flex-direction: column;
  flex: 1;
  justify-content: space-between;
  align-items: center;
}

.pay-notice-content .pn-content .pn-text {
  padding: 0 30rpx;
  box-sizing: border-box;
  font-size: 32rpx;
  color: #333333;
}

.pay-notice-content .pn-content .protocol {
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.pay-notice-content .btn-group {
  width: 100%;
  display: flex;
  justify-content: space-around;
}

.pay-notice-content .btn-group {
  padding: 0 60rpx;
  box-sizing: border-box;
}

.pay-notice-content .btn-group .btn {
  height: 60rpx;
  padding: 0 60rpx;
  border: 1px solid #eeeeee;
  border-radius: 8rpx;
  color: #333333;
  line-height: 56rpx;
  box-sizing: border-box;
  text-align: center;
  margin: 40rpx 0;
  font-size: 28rpx;
}

.pay-notice-content .btn-group .btn.confirm {
  background-image: linear-gradient(90deg, #FF524E 0%, #FB2C2A 100%);
  color: #ffffff;
}

.protocol {
  margin-top: 20rpx;
  font-size: 28rpx;
  color: #666666;
  position: relative;
  display: flex;
  line-height: 44rpx;
  flex-shrink: 0;
}

.protocol-item {
  color: #2B2424;
  font-weight: 500;
}

.protocol .checkbox {
  height: 34rpx;
  width: 34rpx;
  background: #e0e0e0;
  border-radius: 8rpx;
  margin-top: 4rpx;
  margin-left: 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 20rpx;
  border: 1rpx solid #E0E0E0;
  flex-shrink: 0;
}

.protocol .checkbox image {
  display: none;
}

.protocol .checkbox.active {
  border: 1rpx solid #E0E0E0;
  background: #ffffff;
}

.protocol .checkbox.active image {
  display: inline-block;
  width: 24rpx;
  height: 20rpx;
}
/* 支付提示 notice */

/* 选择支付方式 */
.pay-method-layer {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9901;
  background: rgba(0,0,0,0.20);
  height: 100%;
  width: 750rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-end;
}

.pay-method-layer .pay-method-container {
  background: #ffffff;
  display: flex;
  flex-direction: column;
  width: 750rpx;
  min-height: 924rpx;
  overflow: hidden;
}

.pay-method-layer .pay-method-container .name{
  color: #5B5454;
  margin-top: 75rpx;
  margin-bottom: 50rpx;
  text-align: center;
}

.pay-method-layer .pay-method-container .number{
  text-align: center;
  color: #5B5454;
}

.pay-method-layer .pay-method-item .icon {
  height: 52rpx;
  width: 52rpx;
  display: flex;
}

.pay-method-layer .checkbox-icon {
  height: 40rpx;
  width: 56rpx;
  display: flex;
}

.pay-method-layer .pay-method-container .pm-header {
  height: 108rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #2B2424;
  font-size: 32rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #F7F7F7;
  position: relative;
}

.pay-method-layer .pm-header .close {
  height: 50rpx;
  width: 50rpx;
  position: absolute;
  left: 36rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.pay-method-layer .pm-header .close .close-icon {
  width: 25rpx;
  height: 25rpx;
  position: relative;
}

.pay-method-layer .pm-header .close .close-icon::before {
  width: 28rpx;
  height: 3rpx;
  background: #000000;
  position: absolute;
  content: '';
  display: inline-block;
  top: 10rpx;
  transform: rotate(45deg);
}

.pay-method-layer .pm-header .close .close-icon::after {
  width: 28rpx;
  height: 3rpx;
  background: #000000;
  position: absolute;
  content: '';
  display: inline-block;
  top: 10rpx;
  transform: rotate(-45deg);
}

.pay-method-layer .pay-method-content {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.pay-method-layer .pay-method-content .total {
  display: inline-block;
  height: 192rpx;
  line-height: 192rpx;
  font-size: 60rpx;
  color: #2B2424;
  width: 100%;
  text-align: center;
  border-bottom: 1rpx solid #f7f7f7;
}

.pay-method-layer .pay-method-content .balance{
  margin-top: 30rpx;
} 

.pay-method-layer .pay-method-item {
  width: 100%;
  height: 144rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding-left: 48rpx;
  justify-content: space-between;
}

.pay-method-layer .pay-method-item .item-main {
  width: 608rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #f7f7f7;
  height: 144rpx;
}

.pay-method-layer .pay-method-item .item-main .pay-menthod-checkbox {
  height: 44rpx;
  width: 44rpx;
  border-radius: 50%;
  box-sizing: border-box;
  border: 1rpx solid #F64C47;
  margin-right: 48rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.pay-method-layer .pay-method-item .item-main .pay-menthod-checkbox.active {
  background: #F64C47;
}

.pay-method-layer .bottom-btns {
  position: absolute;
  bottom: 0;
  width: 100%;
  display: flex;
  justify-content: center;
  padding-bottom: 64rpx;
}

.pay-method-layer .bottom-btns .pay-method-confirm {
  background-image: linear-gradient(118deg, #FF524E 0%, #FB2C2A 100%);
  border-radius: 42rpx;
  border-radius: 42rpx;
  height: 84rpx;
  width: 654rpx;
  display: inline-block;
  line-height: 84rpx;
  text-align: center;
  font-size: 36rpx;
  color: #ffffff;
}
/* 选择支付方式 */

/* 售后列表 */
.sale-services-container {
  display: flex;
  flex-direction: column;
