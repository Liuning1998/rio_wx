/* 余额支付结果弹窗 */
.balance-result{
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background-color: #f7f7f7;
  z-index: 200;
}
.balance-result .subject{
  width: 100%;
  background-color: #fff;
  overflow: hidden;
  padding: 96rpx 0 0;
}
.balance-result .subject .img{
  width: 127.6rpx;
  height: 127.6rpx;
  display: block;
  margin: 0 auto;
}
.balance-result .subject .result{
  text-align: center;
  margin: 24.4rpx 0 0;
  font-size: 36rpx;
  color: #342F27;
}
.balance-result .subject .money{
  font-size: 60rpx;
  text-align: center;
  color: #2B2424;
}
.balance-result .subject .info{
  padding: 0 24rpx;
  margin-top: 66rpx;
  border-top: 1px solid #f7f7f7;
}
.balance-result .subject .info .info-l{
  color: #5B5454;
  display: flex;
  margin: 48rpx 0;
  font-size: 24rpx;
  justify-content: space-between;
  color: #666666;
  line-height: 24rpx;
}
.balance-result .subject .info .info-l view:nth-child(1){
  margin-right: 20rpx;
  flex: none;
}
.balance-result .subject .info .info-l view:nth-child(2){
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.balance-result .btn-box{
  display: flex;
  justify-content: space-between;
  padding: 0 32rpx;
}
.balance-result .btn-box .btn{
  width: 328rpx;
  line-height: 88rpx;
  text-align: center;
  border-radius: 48rpx;
  border: 1px solid #F64C47;
  font-size: 32rpx;
  color: #F64C47;
  letter-spacing: 0;
  text-align: center;
  margin: 64rpx 0;
}
.balance-result .btn-box .btn.btn-order{
  background-image: linear-gradient(90deg, #FF524E 0%, #FB2C2A 100%);
  border: none;
  color: #fff;
}
