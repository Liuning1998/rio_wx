/* pages/orders/show/index.wxss */
page {
  background: #f7f7f7;
}

.page-container {
  height: 100%;
  /* flex: 1; */
  overflow-y: scroll;
}

.page-content {
  padding-bottom: 240rpx;
}

.page-content.iphone-x {
  padding-bottom: 360rpx;
}

.order-item-container {
  background: #ffffff;
  width: 750rpx;
  box-sizing: border-box;
  padding: 30rpx 20rpx;
  overflow: hidden;
  margin-top: 12rpx;
}

.order-header {
  width: 750rpx;
  height: 192rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 48rpx;
  position: relative;
}

.order-header .background {
  height: 180rpx;
  width: 128rpx;
  position: absolute;
  z-index: 5;
  height: 192rpx;
  width: 750rpx;
}

.order-header .status-container {
  height: 192rpx;
  width: 100%;
  padding: 0 24rpx;
  box-sizing: border-box;
  position: relative;
  z-index: 7;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.order-header .status-container .icon {
  width: 32rpx;
  height: 32rpx;
  display: flex;
  margin-right: 20rpx;
}

.order-header .status-container .status-content {
  display: flex;
  font-size: 32rpx;
  color: #ffffff;
  font-weight: 500;
  align-items: center;
}

.order-header .status-container .desc {
  margin-top: 10rpx;
  color: #ffffff;
  font-size: 24rpx;
}

.products {
  width: 100%;
  border-top-left-radius: 8rpx;
  border-top-right-radius: 8rpx;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}

.products .product-item {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-bottom: 24rpx;
}

.products .product-item:last-child {
  border-bottom: none;
  margin-bottom: 0;
}

.products .product-item .avatar-container {
  position: relative;
  margin-right: 16rpx;
}

.products .product-item .avatar-container .sale-service-status {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 112rpx;
  height: 32rpx;
  background: #f00;
  color: #ffffff;
  font-size: 20rpx;
  line-height: 32rpx;
  text-align: center;
}

.products .product-item .avatar-container .sale-service-status.back-green{
  background-color: #77D9BD;
}

.products .product-item .avatar {
  height: 112rpx;
  width: 112rpx;
  flex-shrink: 0;
}

.products .product-item .product-info {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  width: 100%;
  height: 112rpx;
  justify-content: space-between;
}

.products .product-item .product-info .name-quantity {
  display: flex;
  width: 100%;
  flex-direction: column;
}

.products .product-item .product-info .name {
  font-size: 24rpx;
  color: #5B5454 ;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  max-height: 80rpx;
  width: 560rpx;

  display: -webkit-box;
	white-space: normal !important;
	-webkit-line-clamp: 2;
  line-clamp: 2;
	-webkit-box-orient: vertical;
}

.products .product-item .product-info .variant-desc {
  font-size: 20rpx;
  color: #B4AFAF;
}

.products .product-item .product-info .variant-info {
  font-size: 24rpx;
  color: #666666;
  margin-top: 10rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.products .product-item .product-info .price-quantity {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 100%;
}

.products .product-item .product-info  .price {
  font-size: 24rpx;
  color: #2B2424;
}

.products .product-item .product-info .quantity {
  font-size: 24rpx;
  color: #2B2424;
}

/* 费用信息 */
.product-expenses, .shipment-expenses {
  height: 88rpx;
  width: 750rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #2B2424;
  font-size: 24rpx;
  background: #ffffff;
  /* margin-top: 1px; */
}

.product-expenses .content, .shipment-expenses .content {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-top: 1rpx solid #f7f7ff;
}

.product-expenses.vip-info .name{
  padding: 0 12rpx;
  border-radius: 4rpx;
  background-color: #F64C47;
  color: #fff;
  font-size: 22rpx;
  height: 30rpx;
  line-height: 30rpx;
  display: inline-block;
  margin-left: 8rpx;
  position: relative;
  top: -1rpx;
}

.quantity-and-total {
  height: 100rpx;
  width: 750rpx;
  box-sizing: border-box;
  padding: 0 24rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #ffffff;
}

.quantity-and-total .remark image {
  height: 24rpx;
  width: 24rpx;
  display: flex;
  margin-right: 8rpx;
  flex-shrink: 0;
}

.quantity-and-total .remark {
  display: flex;
  align-items: center;
  color: #B4AFAF;
  font-size: 24rpx;
  flex-shrink: 0;
}

.quantity-and-total .content {
  /* width: 100%; */
  height: 100%;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  border-top: 1rpx solid #f7f7ff;
  flex: 1;
}

.quantity-and-total .quantity {
  color: #B4AFAF;
  font-size: 24rpx;
  margin-right: 24rpx;
  margin-top: 1px;
}

.quantity-and-total .total-label {
  font-size: 28rpx;
  color: #2B2424;
  margin-trim: 8rpx;
}

.quantity-and-total .total {
  font-size: 32rpx;
  color: #F64C47;
}
/* 费用信息 */

.total-info {
  display: flex;
  justify-content: flex-end;
  color: #666666;
  font-size: 24rpx;
  margin-top: 10rpx;
}

.total-info .total {
  margin-left: 8rpx;
}

.total-info .price {
  color: #e23c42;
  margin: 0 2rpx;
}

.actions {
  display: flex;
  align-items: center;
  justify-content: flex-end;
  margin-top: 20rpx;
}

.buy-btn {
  height: 44rpx;
  width: 136rpx;
  box-sizing: border-box;
  border: 1rpx solid #e23c42;
  border-radius: 22rpx;
  color: #e23c42;
  text-align: center;
  line-height: 42rpx;
  font-size: 24rpx;
}

/* 快递信息 */
.status-and-express .status-line {
  display: flex;
  width: 750rpx;
  padding: 24rpx;
  box-sizing: border-box;
  min-height: 132rpx;
  align-items: center;
  background: #ffffff;
}

.status-and-express .status-line .icon {
  width: 30rpx;
  height: 32rpx;
  display: flex;
  flex-shrink: 0;
  margin-right: 24rpx;
}

.status-and-express .status-line .desc {
  font-size: 28rpx;
  color: #2B2424;
  font-weight: 500;
}

.expresses-container .icon {
  width: 32rpx;
  height: 24rpx;
  display: flex;
  flex-shrink: 0;
  margin-right: 24rpx;
}

.expresses-container .one {
  display: flex;
  align-items: center;
  width: 750rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
  height: 128rpx;
  background: #ffffff;
}

.expresses-container .one .content {
  display: flex;
  flex: 1;
  flex-direction: column;
}

.expresses-container .one .title {
  font-size: 28rpx;
  color: #2B2424;
  font-weight: 500;
}

.expresses-container .one .info {
  font-size: 20rpx;
  color: #2B2424;
}

.expresses-container .more {
  display: flex;
  align-items: center;
  width: 750rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
  height: 128rpx;
  background: #ffffff;
  font-size: 28rpx;
  color: #2B2424;
  font-weight: 500;
}

.expresses-container .more .content {
  flex: 1;
}

.expresses-container .more .arrow-down {
  transform: rotate(90deg);
}

.expresses-container .more .arrow-down.extend {
  transform: rotate(-90deg);
}

.expresses-container .expresses-list {
  padding-bottom: 24rpx;
  background: #ffffff;
}

.expresses-container .expresses-list .info {
  display: flex;
  height: 64rpx;
  padding: 0 80rpx;
  background: #ffffff;
  box-sizing: border-box;
  align-items: center;
  font-size: 24rpx;
  color: #2B2424;
  font-weight: 400;
}

.expresses-container .expresses-list .info .no {
  margin-left: 24rpx;
  flex: 1;
}
/* 快递信息 */

/* 地址信息 */
.select-address-container .address-icon, .select-address-empty .location {
  height: 32rpx;
  width: 32rpx;
  display: flex;
  flex-shrink: 0;
  padding: 0 24rpx;
}

.select-address-container .arrow, .select-address-empty .arrow {
  width: 12rpx;
  height: 26rpx;
  transform: rotate(180deg);
  flex-shrink: 0;
  padding: 0 20rpx;
}

.split-line {
  height: 8rpx;
  width: 750rpx;
}

.select-address-container, .select-address-empty {
  display: flex;
  width: 750rpx;
  box-sizing: border-box;
  padding: 28rpx 0;
  align-items: center;
  background: #ffffff;
  margin-top: 1px;
}

.select-address-empty text {
  display: flex;
  flex: 1;
  font-size: 28rpx;
  color: #2B2424;
  font-weight: 500;
}

.select-address-container .address-content {
  min-height: 76rpx;
  flex: 1;
}

.select-address-container .user-info {
  display: flex;
  align-items: center;
}

.select-address-container .user-name {
  color: #2B2424;
  font-size: 28rpx;
  font-weight: 500;
  margin-right: 32rpx;
  max-width: 360rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  display: -webkit-box;
}

.select-address-container .phone {
  color: #2B2424;
  font-size: 28rpx;
  font-weight: 500;
}

.select-address-container .address-info {
  font-size: 20rpx;
  color: #2B2424;
}

.select-address-container .address-label {
  margin-right: 8rpx;
}
/* 地址信息 */

