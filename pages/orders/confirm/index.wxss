/* 优惠券start */
.coupon{
  width: 702rpx;
  /* height: 220rpx; */
  margin: 24rpx auto 0;
  display: flex;
  position: relative;
  overflow: hidden;
}
.coupon > .coupon-bg{
  position: absolute;
  left: 50%;
  top: 50%;
  width: 718rpx;
  height: 236rpx;
  transform: translate(-50%, -50%);
}
.coupon .coupon-card{
  position: relative;
}
.coupon .coupon-card > .coupon-card-bg{
  width: 220rpx;
  height: 220rpx;
  position: relative;
  top: 50%;
  transform: translateY(-50%);
}
.coupon .coupon-card .coupon-card-content{
  position: absolute;
  width: 100%;
  top: 45%;
  left: 0;
  transform: translateY(-50%);
  text-align: center;
}
.coupon .coupon-card .coupon-money text:nth-child(1){
  font-size: 20rpx;
  color: #fff;
}
.coupon .coupon-card .coupon-money text:nth-child(2){
  font-size: 60rpx;
  color: #fff;
  font-weight: 500;
}
.coupon .coupon-card .coupon-brief{
  width: 100%;
  font-size: 24rpx;
  line-height: 34rpx;
  color: #fff;
}
.coupon-operation{
  width: calc(100% - 220rpx);
  display: flex;
  position: relative;
  z-index: 1;
  align-content: space-between;
  flex-wrap: wrap;
}
.coupon-operation .coupon-operation-top{
  padding: 32rpx 20rpx 0 26rpx;
  width: 100%;
  box-sizing: border-box;
}
.coupon-operation .coupon-operation-top > text {
  width: 100%;
  font-size: 32rpx;
  color: #342F27;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  display: block;
}
.coupon-operation .coupon-operation-top .coupon-operation-top-btn{
  width: 100%;
  display: flex;
