/* 底部按钮 */
.bottom-tools {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
  background-color: white;
  position: fixed;
  z-index: 199;
  bottom: 0;
  left: 0;
  width: 100%;
  padding: 0 0 0 24rpx;
  box-sizing: border-box;
}

.bottom-tools.iphone-x {
  padding-bottom: 68rpx;
  height: 169rpx;
}

.bottom-tools button {
  border-radius: 0;
  padding: 0;
  margin: 0 !important;
  border: none;
  width: 240rpx;
  height: 96rpx;
  background: #FF3633;
  line-height: 96rpx;
  font-family: PingFangSC-Medium;
  font-size: 36rpx;
  color: #FFFFFF;
  font-weight: 500;
}

.bottom-tools .total {
  margin-right: 8rpx;
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #332B2B;
  font-weight: 400;
}

.bottom-tools .total .price {
  font-family: PingFangSC-Medium;
  font-size: 56rpx;
  color: #FF3633;
  font-weight: 500;
}

.bottom-tools .total .decimal {
  font-family: PingFangSC-Medium;
  font-size: 40rpx;
  color: #FF3633;
  font-weight: 500;
}

.bottom-tools .total .decimal::before{
  content: '.';
}

.bottom-tools .total .unit{
  font-family: PingFangSC-Medium;
  font-size: 36rpx;
  color: #FF3633;
  font-weight: 500;
}

