/* pages/products/buy/index.wxss */
page {
  background: #f7f7f7;
}

.hide {
  display: none !important;
}

.page-container {
  height: 100%;
  /* flex: 1; */
  overflow-y: scroll;
  padding-bottom: 174rpx;
  box-sizing: border-box;
}

.page-content {
  display: flex;
  flex-direction: column;
  position: relative;
}

.main-container {
  box-sizing: border-box;
  border-top-left-radius: 8rpx;
  border-top-right-radius: 8rpx;
  overflow: hidden;
  margin-bottom: 240rpx;
  position: relative;
  z-index: 2;
  display: flex;
  flex-direction: column;
  background-color: #fff;
}

.time-remark {
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  font-size: 40rpx;
  line-height: 1;
  font-weight: 500;
  font-family: PingFangSC-Medium;
  padding: 70rpx 0 88rpx;
  position: relative;
}

/* 地址 */

.select-address-empty .title{
  font-family: PingFangSC-Medium;
  font-size: 36rpx;
  color: #807474;
  margin-bottom: 20rpx;
}

.select-address-empty .btn{
  background: #FF3633;
  border-radius: 12rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 630rpx;
  height: 100rpx;
  font-family: PingFangSC-Medium;
  font-size: 40rpx;
  color: #FFFFFF;
}

.select-address-empty .btn text{
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 2rpx solid #fff;
  padding-bottom: 5px;
  box-sizing: border-box;
  margin-right: 12rpx;
}

.select-address-container .arrow {
  width: 16rpx;
  height:32rpx;
  /* flex-shrink: 0; */
  margin-left: 34rpx;
}

.select-address-container {
  display: flex;
  width: 702rpx;
  box-sizing: border-box;
  padding: 36rpx;
  align-items: center;
  background: #ffffff;
  box-shadow: 0 16rpx 32rpx -30rpx #CCBFBF;
  margin: 0 auto 42rpx;
  border-radius: 12rpx;
  position: relative;
}

.select-address-empty{
  width: 702rpx;
  box-sizing: border-box;
  padding: 36rpx;
  background: #ffffff;
  box-shadow: 0 16rpx 32rpx -30rpx #CCBFBF;
  margin: 0 auto;
  border-radius: 12rpx;
}

.select-address-container .address-content {
  min-height: 76rpx;
  flex: 1;
}

.select-address-container .user-info {
  display: flex;
  align-items: center;
}

.select-address-container .user-name {
  color: #332B2B;
  font-size: 36rpx;
  margin-right: 24rpx;
  max-width: 360rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  display: -webkit-box;
  font-family: PingFangSC-Regular;
  font-weight: 400;
}

.select-address-container .phone {
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #332B2B;
  line-height: 18px;
  font-weight: 400;
}

.select-address-container .address-info {
  font-family: PingFangSC-Medium;
  font-size: 40rpx;
  color: #332B2B;
  font-weight: 500;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-break: break-all;
}

.select-address-container .address-info .default{
  height: 40rpx;
  background: #19A0FF;
  border-radius: 24rpx;
  line-height: 40rpx;
  padding: 0 14rpx;
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #FFFFFF;
  font-weight: 100;
  margin-right: 20rpx;
  top: -4rpx;
  position: relative;
  box-sizing: border-box;
}

.select-address-container .address-label {
  margin-right: 8rpx;
}
/* 地址 */

/* 商品 */

.products .avatar{
  width: 216rpx;
  height: 216rpx;
  flex-shrink: 0;
  margin-right: 18rpx;
  border: 1rpx solid rgba(230,230,230,1);
}

.products {
  min-height: 208rpx;
  width: 750rpx;
  background: #ffffff;
  display: flex;
  align-items: center;
  padding: 16rpx 24rpx;
  box-sizing: border-box;
  margin-top: 1px;
  position: relative;
  flex-direction: column;
}

.products .product-item {
  display: flex;
  width: 100%;
  margin-bottom: 48rpx;
}

.products .product-info {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.products .name-variant {
  display: flex;
  flex-direction: column;
  margin-bottom: 10rpx;
  padding-top: 8rpx;
}

.products .name {
  font-size: 36rpx;
  color: #332B2B;
  max-width: 510rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  display: -webkit-box;
  text-align: justify;
  font-family: PingFangSC-Regular;
  font-weight: 400;
}

.products.products .variant-desc {
  font-size: 24rpx;
  color: #B4AFAF;
  text-align: justify;
}

.products .quantity-price {
  width: 100%;
  display: flex;
  justify-content: space-between;
}


.products .unit{
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #332B2B;
  font-weight: 400;
}

.products .price {
  margin-right: 8rpx;
  font-family: PingFangSC-Medium;
  font-size: 40rpx;
  color: #332B2B;
  font-weight: 500;
}

.products .origin-price {
  font-size: 20rpx;
  color: #B4AFAF;
  text-decoration: line-through;
}

.products .quantity {
  font-size: 24rpx;
  color: #2B2424;
}

/* 商品 */

/* 金额 */
.expenses {
  background: #ffffff;
  width: 702rpx;
  margin: 0 auto;
  border-top: 1rpx solid rgba(230,230,230,1);
}

.expenses .product-expenses {
  width: 100%;
  display: flex;
  box-sizing: border-box;
  align-items: center;
  justify-content: space-between;
  margin: 44rpx 0;
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #332B2B;
  font-weight: 400;
}

.expenses .product-expenses .total-container .unit{
  font-family: PingFangSC-Regular;
  font-size: 40rpx;
  color: #332B2B;
  font-weight: 400;
}

.expenses .product-expenses .total-container .price{
  font-family: PingFangSC-Medium;
  font-size: 40rpx;
  color: #332B2B;
  font-weight: 500;
}

.expenses .shipment-expenses {
  height: 88rpx;
  width: 100%;
  display: flex;
  padding: 0 24rpx;
  box-sizing: border-box;
  align-items: center;
  justify-content: space-between;
  color: #FF3633;
  font-size: 24rpx;
}

.expenses .discount-shipment-expenses {
  height: 88rpx;
  width: 100%;
  display: flex;
  padding: 0 24rpx;
  box-sizing: border-box;
  align-items: center;
  justify-content: space-between;
  color: #2B2424;
  font-size: 24rpx;
}

.expenses .discount-shipment-expenses .price {
  color: #FB2C2A;
}

.balance-container {
  width: 750rpx;
  padding: 0 24rpx;
  height: 128rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #ffffff;
  margin-top: 16rpx;
}

.balance-container .balance-info {
  display: flex;
  flex-direction: column;
}

.balance-container .balance-info .label {
  font-size: 28rpx;
  color: #2B2424 ;
}

.balance-container .balance-info .content {
  font-size: 20rpx;
  color: #2B2424;
}
