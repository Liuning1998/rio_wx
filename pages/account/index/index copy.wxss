/* pages/account/index/index.wxss */

page {
  background: #ffffff;
}

.navbar {
  width: 750rpx;
  position: fixed;
  z-index: 90;
  overflow: hidden;
  /* version1.2 s */
  display: none; 
  /* version1.2 e */
}

.navbar image {
  position: absolute;
  z-index: 91;
  left: 0;
  top: 0;
  width: 750rpx;
  height: 352rpx;
}

.navbar text {
  position: relative;
  z-index: 92;
  width: 750rpx;
  text-align: center;
  color: #8E662F; 
  font-weight: 600;
  font-size: 36rpx;
  display: inline-block;
  height: 44px;
  line-height: 44px;
}

.page-container {
  height: 100%;
  /* flex: 1; */
  overflow-y: scroll;
}

.page-content {
  padding-bottom: 240rpx;
}

.page-content.iphone-x {
  padding-bottom: 360rpx;
}

.page-header {
  width: 750rpx;
  height: 418rpx;
  position: relative;
  display: flex;
  flex-direction: column;
  /* version1.2 s */
  display: none;
  /* version1.2 e */
}

.page-header .header-background-container {
  position: relative;
  z-index: 1;
}

.page-header .header-background-container image {
  height: 418rpx;
  width: 750rpx;
  display: flex;
}

.page-header .header-content {
  height: 418rpx;
  position: absolute;
  z-index: 3;
  top: 0;
  left: 0;
  width: 750rpx;
  display: flex;
  flex-direction: column;
}

.user-container {
  width: 750rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  flex: 1;
}

.user-content {
  width: 100%;
  display: flex;
  align-items: center;
  padding: 0 40rpx;
  box-sizing: border-box;
}

.user-content .welcome-info {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}

.user-content .welcome-info .user-name{
  color: #ffffff;
  font-size: 28rpx;
  display: flex;
  font-weight: 500;
  width: 100%;
}

.user-content .welcome-info .user-name .name {
  max-width: 360rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  display: -webkit-box;
  box-sizing: border-box;
  margin-right: 50rpx;
}

.user-content .welcome-info .mark {
  font-size: 23rpx;
  color: #ffffff;
  height: 32rpx;
  line-height: 32rpx;
  padding: 0rpx 12rpx;
  display: inline-block;
  border-radius: 16rpx;
  border: 1px solid #ffffff;
  /* box-sizing: border-box; */
  font-weight: 500;
}
.user-content .welcome-info .user-member-leval{
  border-radius: 16px;
  margin-left: 8rpx;
  display: flex;
  position: relative;
  align-items: center;
  background-image: linear-gradient(154deg, #FFFFFF 0%,#FFE6A2 100%);
  padding: 1px;
}
.user-content .welcome-info .user-member-leval text{
  line-height: 32rpx;
  display: inline-block;
  border-radius: 16px;
  padding: 0 14rpx;
  font-size: 23rpx;
  color: #FFFFFF;
  z-index: 1;
  background-image: linear-gradient(154deg, #FADF4C 0%, #F8C042 26%, #FDAE31 100%);
  max-width: 219rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.user-content .welcome-info .user-member-leval .bg{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}

.user-other{
  display: flex;
}

.no-login {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.no-login .default-avatar {
  margin-right: 24rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.no-login .default-avatar image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background: #ffffff;
}

.no-login .no-login-desc {
  display: flex;
  flex-direction: column;
}

.no-login .item-label {
  font-size: 36rpx;
  color: #ffffff;
  font-weight: 500;
}

.no-login .sub-item-label {
  font-size: 20rpx;
  color: #ffffff;
}

.user-content .avatar-container {
  height: 100rpx;
  width: 100rpx;
  border-radius: 50%;
  overflow: hidden;
  /* border: 1px solid #ffffff; */
  flex-shrink: 0;
  margin-right: 24rpx;
  padding: 0;
  background-color: rgba(0, 0, 0, 0);
}
.avatar-containe-parent  .pendant{
  width: 36rpx;
  height: 32rpx;
  position: absolute;
  top: -13rpx;
  left: -3rpx;
}
.avatar-containe-parent{
  position: relative;
}
.user-content .avatar-container .avatar-image {
  height: 100rpx;
  width: 100rpx;
  border-radius: 50%;
  overflow: hidden;
}

.action-container .icon-01{
  width: 80rpx;
  height: 80rpx;
  display: flex;
  margin-bottom: 6rpx;
}

.action-container .icon-02{
  width: 52rpx;
  height: 52rpx;
  display: flex;
  margin-bottom: 6rpx;
}

.action-container {
  display: flex;
  flex-direction: column;
  width: 702prx;
  margin-left: 24rpx;
  margin-top: 40rpx;
  margin-bottom: 32rpx;
  /* version1.2 s */
  display: none;
  /* version1.2 e */
}

.action-container .label {
  font-size: 28rpx;
  color: #2B2424;
  margin-bottom: 32rpx;
}

.action-container .action-content {
  display: flex;
  width: 702rpx;
  justify-content: space-around;
}

.action-container .action-content .action-item{
  display: flex;
  flex-direction: column;
  align-items: center;
}

.action-container .action-content .item-label {
  font-size: 24rpx;
  color: #5B5454;
}

/* 订单 */
.order-btn-container .btn-icon, .account-info .main-btn .btn-icon {
  width: 40rpx;
  height: 40rpx;
  display: flex;
  margin-bottom: 8rpx;
}

.order-btn-container {
  display: flex;
  height: 160rpx;
  width: 750rpx;
  /* version1.2 s */
  display: none;
  /* version1.2 e */
}

.order-btn-container .btn-group {
  flex: 1;
  display: flex;
  justify-content: space-around;
}

.order-btn-container .btn-item, .account-info .main-btn .btn-item {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: relative;
}

.order-btn-container .label, .account-info .main-btn .label {
  font-size: 24rpx;
  color: #5B5454;
}

.order-btn-container .split-icon, .account-info .split-icon {
  height: 160rpx;
  width: 16rpx;
  display: flex;
  flex-shrink: 0;
}

.order-btn-container .main-btn, .account-info .main-btn {
  width: 184rpx;
  flex-shrink: 0;
}

.order-btn-container .btn-badge {
  position: absolute;
  right: 6rpx;
  top: 30rpx;
  background: #F64C47;
  color: #ffffff;
  font-size: 16rpx;
  display: inline-block;
  height: 22rpx;
  line-height: 22rpx;
  border-radius: 11rpx;
  padding: 0 6rpx;
  min-width: 22rpx;
  box-sizing: border-box;
  text-align: center;
}

.account-info {
  height: 160rpx;
  width: 750rpx;
  display: flex;
  /* version1.2 */
  display: none;
  /* version1.2 */
}

.account-info .account-content {
  flex: 1;
  display: flex;
  justify-content: space-around;
}

.account-info .info-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.account-info .info-item .label {
  font-size: 24rpx;
  color: #5B5454;
}

.account-info .info-item .desc {
  font-size: 36rpx;
  color: #2B2424;
  font-weight: 500;
  line-height: 48rpx;
}
/* 订单 */

/* 会员权益 */
.member{
  width: 702rpx;
  margin: 50.2rpx auto 0;
  height: 88rpx;
  position: relative;
  display: flex;
  justify-content: space-between;
  padding: 0 25rpx 0 24rpx;
  box-sizing: border-box;
}
.member .member-back{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
}
.member .member-left{
  width: calc(100% - 150rpx);
  display: flex;
  align-items: center;
  position: relative;
}
.member .member-logo{
  width: 132rpx;
  height: 28rpx;
}
.member .member-des{
  font-size: 24rpx;
  color: #F6D876;
  text-align: center;
  margin-left: 44.8rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
.member .member-go{
  display: flex;
  align-items: center;
  position: relative;
}
.member .member-go text{
  font-size: 22rpx;
  color: #Fff;
}
.member .member-go image{
  width: 8rpx;
  height: 16rpx;
  transform: rotate(180deg);
  margin-left: 11rpx;
}
/* 会员权益 end */



