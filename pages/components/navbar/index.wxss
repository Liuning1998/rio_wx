/* pages/components/navbar/index.wxss */
.navbar-container {
  display: flex;
  flex-direction: column;
  background: #f7f7f7;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 999;
  width: 100%;
}

.navbar-container-dark {
  display: flex;
  flex-direction: column;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 99990;
  width: 100%;
  overflow: hidden;
}

.dark_bg{
  position: absolute;
  top: 0;
  width: 100%;
  left: 0;
}

.navbar-content {
  width: 100%;
  height: 44px;
  line-height: 44px;
  display: flex;
  position: relative;
  justify-content: center;
  align-items: center;
  z-index: 1;
}

.return-image-container {
  position: absolute;
  left: 0;
  top: 0rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 44px;
  padding-left: 30rpx;
  padding-right: 60rpx;
  z-index: 99992;
}

.return-image {
  height: 36rpx;
  width: 20rpx;
}

.navbar-content .navbar-title {
  font-size: 36rpx;
  height: 44px;
  line-height: 44px;
  font-weight: 500;
}

.navbar-content .navbar-title {
  color: #342F27;
}

.navbar-content .navbar-title.text-white {
  color: #fff;
}